    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "orm:init": "typeorm-ts-node-esm -d ./src/config/data.source.ts",
    "m:gen:dev": "npm run orm:init migration:generate",
    "m:run:dev": "npm run orm:init migration:run",
    "m:gen": "npm run orm:init migration:generate",
    "m:run": "npm run orm:init migration:run",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker-compose rm -s -f -v billete_pg",
    "db:dev:up": "docker-compose up -d billete_pg",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && npm run prisma:dev:deploy",
    "generate-deploy-scripts": "node src/utils/generateGraphDeployScripts.js",
    "deploy:subgraph:goerli-5": "graph deploy --studio goerli-5 C:\\Users\\lsper\\Desktop\\Backend\\billete\\src\\networks\\subgraphs\\goerli-5.yaml",
